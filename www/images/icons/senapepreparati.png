				if (mq.matches) {
					if ($("ul.list__tab__2").hasClass("list__tab__open")) {
						$("ul.list__tab__2").removeClass("list__tab__open");
						$("ul.list__tab__2").addClass("list__tab__close");
					} else {
						$("ul.list__tab__2").addClass("list__tab__open");
					}
				}
			}
			$('#menu__mobile-btn').on('click', function() {
				if ($(this).hasClass('mm-open')) {
					$(this).removeClass('mm-open');
					$('.modal-bg').removeClass('modal-bg-open');
				} else {
					$('#menu__mobile-btn').addClass('mm-open');
					$('.modal-bg').addClass('modal-bg-open');
				}
			});

			$('.openCondividiMobile').on('click', function(){
				if($('.mobile-icn-box-condividi').hasClass('show')) {
					$('.mobile-icn-box-condividi').removeClass('show');
					$('.mobile-icn-box-condividi').addClass('hide');
				} else {
					$('.mobile-icn-box-condividi').removeClass('hide');
					$('.mobile-icn-box-condividi').addClass('show');
				}
			});
			
